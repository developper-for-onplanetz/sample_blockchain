from bs4 import BeautifulSoup
from urllib.request import urlopen

target_url = "https://hrtechnavi.jp/lab/hrtech-chaosmap/"

html = urlopen(target_url)
data = html.read()
html = data.decode('utf-8')

soup = BeautifulSoup(html, 'html.parser')

#HTMLで取得する場合
